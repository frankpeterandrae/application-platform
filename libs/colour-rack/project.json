{
	"name": "colour-rack",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/colour-rack/src",
	"prefix": "lib",
	"projectType": "library",
	"tags": ["scope:web", "type:feature"],
	"targets": {
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/colour-rack/jest.config.ts",
				"tsConfig": "libs/colour-rack/tsconfig.spec.json"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint"
		},
		"stylelint": {
			"executor": "nx-stylelint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["libs/colour-rack/**/*.css", "libs/colour-rack/**/*.scss"]
			}
		}
	}
}
