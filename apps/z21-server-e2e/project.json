{
	"name": "z21-server-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"implicitDependencies": ["z21-server"],
	"tags": ["type:e2e", "scope:z21", "platform:node"],
	"targets": {
		"e2e": {
			"executor": "@nx/vitest:test",
			"outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
			"options": {
				"tsConfig": "apps/z21-server-e2e/tsconfig.spec.json",
				"vitestConfig": "apps/z21-server-e2e/vitest.config.mts",
				"reportsDirectory": "../../coverage/z21-server-e2e",
				"passWithNoTests": true
			},
			"dependsOn": ["z21-server:build"]
		}
	}
}
