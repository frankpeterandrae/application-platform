{
	"name": "z21-server-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"implicitDependencies": ["z21-server"],
	"tags": ["type:e2e", "scope:z21", "platform:node"],
	"targets": {
		"e2e": {
			"executor": "@nx/vitest:test",
			"outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
			"options": {
				"tsConfig": "apps/z21-server-e2e/tsconfig.spec.json",
				"vitestConfig": "apps/z21-server-e2e/vitest.config.mts",
				"reportsDirectory": "../../coverage/z21-server-e2e",
				"passWithNoTests": true
			},
			"dependsOn": ["typecheck-test", "typecheck-e2e", "z21-server:build"],
			"configurations": {
				"ci": {
					"ci": true,
					"executor": "@nx/vitest:test",
					"options": {
						"vitestConfig": "apps/z21-server-e2e/vitest.config.ci.mts"
					}
				}
			}
		},
		"typecheck-e2e": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx tsc -p {projectRoot}/tsconfig.json --noEmit"
			}
		},
		"typecheck-test": {
			"executor": "nx:run-commands",
			"options": {
				"command": "npx tsc -p {projectRoot}/tsconfig.spec.json --noEmit"
			}
		}
	}
}
